      local MiscSection = Misc:NewSection("Misc")
      MiscSection:NewButton("DeleteNoBuildZones", "Delete The NoBuildZones(only for you)", function()
        game.Workspace.Map.NoBuildZones:Destroy()
      end)
      MiscSection:NewButton("Invis Healia", "Makes Healia invis only for u", function()
        game:GetService("Workspace").Replicators.NonPassive["Healing Aura"]:Destroy()
