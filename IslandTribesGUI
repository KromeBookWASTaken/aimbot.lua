        local args = {
            [1] = "Weapon Trader",
            [2] = 16
        }
        
        game:GetService("ReplicatedStorage").References.Comm.Events.TradeTrader:FireServer(unpack(args))
    end)
    SellSection:NewButton("Sell Zen Bow", "Sells Zen Bow", function()
        local args = {
            [1] = "Weapon Trader",
            [2] = 18
        }
        
        game:GetService("ReplicatedStorage").References.Comm.Events.TradeTrader:FireServer(unpack(args))
    end)
